{
  "private": "true",
  "name": "pewview-frontend",
  "version": "0.1.0",
  "main": "src/main.ts",
  "scripts": {
    "lint": "eslint",
    "format": "eslint --fix",
    "stylelint": "stylelint src/**/**/*.css src/**/*.css",
    "check-duplicate-code": "jsinspect -t 20 src",
    "dev": "vite",
    "build": "tsc && vite build",
    "build:watch": "concurrently 'npm:build:watch:ts' 'npm:build:watch:vite'",
    "build:watch:ts": "tsc --watch",
    "build:watch:vite": "vite build --watch",
    "serve": "vite preview"
  },
  "license": "MIT",
  "dependencies": {
    "dat.gui": "^0.7.7",
    "solar-calculator": "^0.3.0",
    "three": "^0.134.0"
  },
  "devDependencies": {
    "@types/dat.gui": "^0.7.7",
    "@types/three": "^0.133.1",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "autoprefixer": "^10.4.0",
    "concurrently": "^6.3.0",
    "eslint": "^8.1.0",
    "postcss": "^8.3.11",
    "prettier": "^2.4.1",
    "typescript": "^4.4.4",
    "vite": "^2.6.13"
  }
}
