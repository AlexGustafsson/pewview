{
  "private": "true",
  "name": "pewview-frontend",
  "version": "0.1.0",
  "main": "src/main.ts",
  "scripts": {
    "lint": "eslint",
    "format": "eslint --fix",
    "stylelint": "stylelint src/**/**/*.css src/**/*.css",
    "check-duplicate-code": "jsinspect -t 20 src",
    "dev": "vite",
    "build": "tsc && vite build",
    "build:watch": "concurrently 'npm:build:watch:ts' 'npm:build:watch:vite'",
    "build:watch:ts": "tsc --watch",
    "build:watch:vite": "vite build --watch",
    "serve": "vite preview"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/dat.gui": "^0.7.7",
    "@types/three": "^0.137.0",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "eslint": "^8.9.0",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5",
    "vite": "^2.8.1"
  },
  "dependencies": {
    "dat.gui": "^0.7.7",
    "solar-calculator": "^0.3.0",
    "three": "^0.137.5"
  }
}
