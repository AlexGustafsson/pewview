{
  "private": "true",
  "name": "pewview-frontend",
  "version": "0.1.0",
  "main": "src/main.ts",
  "scripts": {
    "lint": "prettier --check '**/*.{js,ts,css,html,json}'",
    "format": "prettier --write '**/*.{js,ts,css,html,json}'",
    "dev": "vite",
    "build": "tsc && vite build",
    "build:watch": "concurrently 'npm:build:watch:ts' 'npm:build:watch:vite'",
    "build:watch:ts": "tsc --watch",
    "build:watch:vite": "vite build --watch",
    "serve": "vite preview"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/dat.gui": "^0.7.7",
    "@types/three": "^0.137.0",
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5",
    "vite": "^2.8.1"
  },
  "dependencies": {
    "dat.gui": "^0.7.7",
    "solar-calculator": "^0.3.0",
    "three": "^0.137.5"
  }
}
